  },
  "devDependencies": {
    "@formatjs/intl-getcanonicallocales": "^1.2.6",
    "@types/jest": "^25.2.1",
    "cldr-misc-full": "^36.0.0",
    "jest": "^25.4.0",
    "ts-jest": "^25.4.0"
  },
  "dependencies": {
    "@formatjs/intl-utils": "^3.3.1"
  },
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/intl-listformat.d.ts",
  "files": [
    "dist-es6",
    "dist",
    "lib",
    "polyfill-locales.js",
    "polyfill.js",
    "src"
